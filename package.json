{
  "name": "typescript-template",
  "version": "0.1.0",
  "author": "Adam Boe",
  "description": "A launching point for a TypeScript repository",
  "private": false,
  "repository": {
    "type": "git",
    "url": "https://github.com/aboe026/typescript-template.git"
  },
  "bugs": {
    "url": "https://github.com/aboe026/typescript-template/issues"
  },
  "engines": {
    "node": ">=18.14.2",
    "npm": ">=9.5.0"
  },
  "main": "src/index.ts",
  "scripts": {
    "build": "tsc",
    "clean": "rimraf build",
    "lint": "eslint . && prettier --check .",
    "lint:fix": "eslint --fix . && prettier --write .",
    "start": "ts-node src/index.ts",
    "update-dependencies": "ts-node script/update-dependencies.ts"
  },
  "dependencies": {
    "dotenv": "16.0.3",
    "envalid": "7.3.1"
  },
  "devDependencies": {
    "@typescript-eslint/parser": "5.54.1",
    "@typescript-eslint/eslint-plugin": "5.54.1",
    "eslint": "8.36.0",
    "eslint-config-prettier": "8.7.0",
    "eslint-plugin-prettier": "4.2.1",
    "prettier": "2.8.4",
    "rimraf": "4.4.0",
    "ts-node": "10.9.1",
    "typescript": "4.9.5"
  }
}
