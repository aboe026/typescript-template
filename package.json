{
  "name": "typescript-template",
  "version": "0.1.0",
  "author": "Adam Boe",
  "description": "A launching point for a TypeScript repository",
  "private": false,
  "repository": {
    "type": "git",
    "url": "https://github.com/aboe026/typescript-template.git"
  },
  "bugs": {
    "url": "https://github.com/aboe026/typescript-template/issues"
  },
  "engines": {
    "node": ">=18.14.2",
    "npm": ">=9.5.0"
  },
  "main": "src/index.ts",
  "scripts": {
    "build": "tsc",
    "clean": "rimraf build",
    "lint": "eslint . && prettier --check .",
    "lint-fix": "eslint --fix . && prettier --write .",
    "start": "ts-node src/index.ts",
    "update-dependencies": "ts-node script/update-dependencies.ts"
  },
  "dependencies": {
    "dotenv": "16.4.5",
    "envalid": "8.0.0"
  },
  "devDependencies": {
    "@typescript-eslint/parser": "7.4.0",
    "@typescript-eslint/eslint-plugin": "7.4.0",
    "eslint": "8.57.0",
    "eslint-config-prettier": "9.1.0",
    "eslint-plugin-prettier": "5.1.3",
    "prettier": "3.2.5",
    "rimraf": "5.0.5",
    "ts-node": "10.9.2",
    "typescript": "5.4.3"
  }
}
